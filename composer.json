{
    "name": "sfneal/cruise",
    "description": "Boilerplate for PHP package repo's",
    "keywords": [
        "sfneal",
        "php-package-boilerplate"
    ],
    "homepage": "https://github.com/sfneal/cruise",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Stephen Neal",
            "email": "stephen.neal14@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "sfneal/laravel-helpers": "^2.4",
        "symfony/process": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sfneal\\Cruise\\Providers\\CruiseServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Sfneal\\Cruise\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sfneal\\Cruise\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-package-install": [
            "php artisan cruise:install"
        ],
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
